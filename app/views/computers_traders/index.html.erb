<p id="notice"><%= notice %></p>

<h1>Computers Traders</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @computers_traders.each do |computers_trader| %>
      <tr>
        <td><%= computers_trader.name %></td>
        <td><%= computers_trader.description %></td>
        <td><%= link_to 'Show', computers_trader %></td>
        <td><%= link_to 'Edit', edit_computers_trader_path(computers_trader) %></td>
        <td><%= link_to 'Destroy', computers_trader, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

