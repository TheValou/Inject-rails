<p id="notice"><%= notice %></p>

<h1>Computers Os</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @computers_os.each do |computers_o| %>
      <tr>
        <td><%= computers_o.name %></td>
        <td><%= computers_o.description %></td>
        <td><%= link_to 'Show', computers_o %></td>
        <td><%= link_to 'Edit', edit_computers_o_path(computers_o) %></td>
        <td><%= link_to 'Destroy', computers_o, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

