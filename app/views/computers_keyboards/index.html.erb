<p id="notice"><%= notice %></p>

<h1>Computers Keyboards</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @computers_keyboards.each do |computers_keyboard| %>
      <tr>
        <td><%= computers_keyboard.name %></td>
        <td><%= computers_keyboard.description %></td>
        <td><%= link_to 'Show', computers_keyboard %></td>
        <td><%= link_to 'Edit', edit_computers_keyboard_path(computers_keyboard) %></td>
        <td><%= link_to 'Destroy', computers_keyboard, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Computers Keyboard', new_computers_keyboard_path %>
