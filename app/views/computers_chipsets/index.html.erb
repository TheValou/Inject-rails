<p id="notice"><%= notice %></p>

<h1>Computers Chipsets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @computers_chipsets.each do |computers_chipset| %>
      <tr>
        <td><%= computers_chipset.name %></td>
        <td><%= computers_chipset.description %></td>
        <td><%= link_to 'Show', computers_chipset %></td>
        <td><%= link_to 'Edit', edit_computers_chipset_path(computers_chipset) %></td>
        <td><%= link_to 'Destroy', computers_chipset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Computers Chipset', new_computers_chipset_path %>
