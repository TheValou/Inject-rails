<p id="notice"><%= notice %></p>

<h1>Computers Quests Resps</h1>

<table>
  <thead>
    <tr>
      <th>Questid</th>
      <th>Resp</th>
      <th>Indice</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @computers_quests_resps.each do |computers_quests_resp| %>
      <tr>
        <td><%= computers_quests_resp.questId %></td>
        <td><%= computers_quests_resp.resp %></td>
        <td><%= computers_quests_resp.indice %></td>
        <td><%= link_to 'Show', computers_quests_resp %></td>
        <td><%= link_to 'Edit', edit_computers_quests_resp_path(computers_quests_resp) %></td>
        <td><%= link_to 'Destroy', computers_quests_resp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

