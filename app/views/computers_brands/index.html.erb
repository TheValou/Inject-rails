<p id="notice"><%= notice %></p>

<h1>Computers Brands</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @computers_brands.each do |computers_brand| %>
      <tr>
        <td><%= computers_brand.name %></td>
        <td><%= computers_brand.description %></td>
        <td><%= link_to 'Show', computers_brand %></td>
        <td><%= link_to 'Edit', edit_computers_brand_path(computers_brand) %></td>
        <td><%= link_to 'Destroy', computers_brand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Computers Brand', new_computers_brand_path %>
