<p id="notice"><%= notice %></p>

<h1>Computers Cpus</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @computers_cpus.each do |computers_cpu| %>
      <tr>
        <td><%= computers_cpu.name %></td>
        <td><%= computers_cpu.description %></td>
        <td><%= link_to 'Show', computers_cpu %></td>
        <td><%= link_to 'Edit', edit_computers_cpu_path(computers_cpu) %></td>
        <td><%= link_to 'Destroy', computers_cpu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

